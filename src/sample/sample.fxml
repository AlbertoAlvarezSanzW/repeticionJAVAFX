<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <GridPane GridPane.columnIndex="1">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="botonAmarillo" mnemonicParsing="false" onMouseClicked="#pantalla2" onMouseEntered="#cambiarAmarillo" onMouseExited="#normalidad" prefHeight="25.0" prefWidth="99.0" text="Amarillo" textAlignment="CENTER" />
            <Label fx:id="labelAmarillo" prefHeight="27.0" prefWidth="95.0" text="Soy Amarillo" textAlignment="CENTER" GridPane.columnIndex="1" />
            <Button fx:id="botonAzul" mnemonicParsing="false" onMouseClicked="#pantalla2" onMouseEntered="#cambiarAzul" onMouseExited="#normalidad" prefHeight="22.0" prefWidth="98.0" text="Azul" GridPane.rowIndex="1" />
            <Label fx:id="labelAzul" layoutX="110.0" layoutY="17.0" prefHeight="23.0" prefWidth="98.0" text="Soy Azul" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Button fx:id="botonVerde" mnemonicParsing="false" onMouseClicked="#pantalla2" onMouseEntered="#cambiarVerde" onMouseExited="#normalidad" prefHeight="20.0" prefWidth="99.0" text="Verde" GridPane.rowIndex="2" />
            <Label fx:id="labelVerde" prefHeight="24.0" prefWidth="98.0" text="Soy Verde" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         </children>
      </GridPane>
   </children>
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
</GridPane>
